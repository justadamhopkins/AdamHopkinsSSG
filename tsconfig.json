{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "target": "es2015",
        "jsx": "react",
        "lib": ["dom", "es6", "es2017"]
    },
    "include": [
        "./src/**/*"
    ]
}
